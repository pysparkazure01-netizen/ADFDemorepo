{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-Transformation-1001"
		},
		"ls_azureadlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_azureadlsgen2'"
		},
		"ls_onprem_git_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_onprem_git'"
		},
		"ls_azureadlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shellazureadlsgen2.dfs.core.windows.net"
		},
		"ls_onprem_git_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\saurabh\\Azure\\source_folder\\SH"
		},
		"ls_onprem_git_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "LAPTOP-O6HEMKDH\\saxen"
		},
		"IRSharedonprem_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/03da433b-270e-47ca-9171-ace662579f75/resourcegroups/rg-Shell-1001/providers/Microsoft.DataFactory/factories/ADF-Tutotrials-1001/integrationruntimes/IR-selfHostedAsus"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-Transformation-1001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_azureadlsgen2_accountKey": {
						"value": "[parameters('ls_azureadlsgen2_accountKey')]"
					},
					"ls_onprem_git_password": {
						"value": "[parameters('ls_onprem_git_password')]"
					},
					"ls_azureadlsgen2_properties_typeProperties_url": {
						"value": "[parameters('ls_azureadlsgen2_properties_typeProperties_url')]"
					},
					"IRSharedonprem_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('IRSharedonprem_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"ls_onprem_git_properties_typeProperties_host": {
						"value": "[parameters('ls_onprem_git_properties_typeProperties_host')]"
					},
					"ls_onprem_git_properties_typeProperties_userId": {
						"value": "[parameters('ls_onprem_git_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}